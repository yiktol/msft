{
    "schemaVersion": "1.0",
    "description": "Automatic Domain Join Configuration created by EC2 Console.",
    "runtimeConfig": {
      "aws:domainJoin": {
        "properties": {
          "directoryId": "%directoryId%",
          "directoryName": "%directoryName%",
          "dnsIpAddresses": [
            "%dnsIpAddresses1%",
            "%dnsIpAddresses2%"
          ]
        }
      }
    }
  }